{
  "name": "report-pro",
  "version": "1.0.0",
  "description": "Shopify Reports App - Better Reports alternative",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "dev:client": "cd frontend && npm run dev",
    "dev:tunnel": "node scripts/start-with-ngrok.js",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "cd frontend && npm run build",
    "start": "node dist/server.js",
    "migrate": "node dist/database/migrate.js",
    "setup:env": "node setup-env.js",
    "test": "jest"
  },
  "keywords": ["shopify", "reports", "analytics", "dashboard"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@shopify/shopify-api": "^9.0.0",
    "@shopify/shopify-app-express": "^3.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.5",
    "redis": "^4.6.10",
    "bullmq": "^5.1.0",
    "ioredis": "^5.3.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "date-fns": "^2.30.0",
    "csv-writer": "^1.6.0",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mysql": "^2.15.24",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ngrok": "^5.0.0"
  }
}

